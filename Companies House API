    "#set seaborn plotting aesthetics\n",
    "\n",
    "def barchart_plot(df):\n",
    "    sns.set(style='white',rc={'figure.figsize':(11.7,9.27)})\n",
    "    sns.barplot(x=\"Year of Creation\", y=\"Number of Companies\", hue=\"Sector Name\", data=df)"
   ]
  },
